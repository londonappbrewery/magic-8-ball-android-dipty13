<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <gradient
        android:startColor="#327D32"
        android:centerColor="#327D77"
        android:endColor="#336489"
        android:type="linear"
        android:angle="0"/>
    <corners
        android:radius="45dp"/>
</shape>
</item>
    <item >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">

            <gradient
                android:startColor="#9CBDC1"
                android:centerColor="#3AC1A3"
                android:endColor="#3AC196"
                android:type="linear"
                android:angle="0"/>
            <corners
                android:radius="45dp"/>
        </shape>
    </item>
</selector>